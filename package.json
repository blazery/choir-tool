{
    "name": "koor-tool",
    "version": "0.1.0",
    "author": "Jasper Roosenmaallen",
    "description": "a small tool for mananging choir layouts",
    "private": true,
    "main": "public/electron.js",
    "homepage": "./",
    "scripts": {
        "start": "react-app-rewired start",
        "start-no-browser": "set BROWSER=none && react-app-rewired start",
        "electron-dev": "nf start",
        "electron-prod": "electron .",
        "build": "react-app-rewired build",
        "pack": "electron-builder --dir -mw",
        "dist": "electron-builder -mw",
        "test": "react-app-rewired test",
        "eject": "react-scripts eject",
        "prepack": "yarn build",
        "postinstall": "electron-builder install-app-deps"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "build": {
        "appId": "com.electron.choir-tool",
        "mac": {
            "category": "public.app-category.utilities"
        },
        "files": [
            "build/**/*",
            "node_modules/**/*"
        ],
        "directories": {
            "buildResources": "assets"
        }
    },
    "dependencies": {
        "@types/electron": "^1.6.10",
        "@types/jest": "24.0.23",
        "@types/node": "12.12.8",
        "@types/react": "16.9.11",
        "@types/react-dnd": "^3.0.2",
        "@types/react-dom": "16.9.4",
        "cuid": "^2.1.6",
        "react": "^16.12.0",
        "react-dnd": "^9.4.0",
        "react-dnd-html5-backend": "^9.4.0",
        "react-dom": "^16.12.0",
        "react-scripts": "3.2.0",
        "typescript": "3.7.2"
    },
    "devDependencies": {
        "@babel/plugin-proposal-decorators": "^7.7.0",
        "@fortawesome/fontawesome-free": "^5.11.2",
        "customize-cra": "^0.9.1",
        "electron": "^7.2.4",
        "electron-builder": "^21.2.0",
        "foreman": "^3.0.1",
        "mobx": "^5.15.0",
        "mobx-react": "^6.1.4",
        "prettier": "^1.19.1",
        "react-app-rewire-mobx": "^1.0.9",
        "react-app-rewired": "^2.1.5",
        "tslint": "^5.20.1",
        "tslint-react": "^4.1.0"
    }
}
